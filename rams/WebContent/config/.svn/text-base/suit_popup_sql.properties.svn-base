Querys.suit.SetOwnerFixedModify = \
UPDATE OWNER SET OWNRATE = ? WHERE AFFAIRNUM = ? AND OWNERNUM = ? \

Querys.suit.SetFilePopAdd = \
INSERT INTO LAWSUIT_FILE(FILEID,AFFAIRNUM,PRESENTDAY,CONTENT,FILENAME,FILELOC,REGISTDATE,FILECATEG) \
VALUES((SELECT MAX(FILEID)+1 FROM LAWSUIT_FILE),?,?,?,?,?,SYSDATE,?) \

Querys.suit.SetFilePopDel = \
DELETE FROM LAWSUIT_FILE WHERE AFFAIRNUM = ? AND FILEID = ? \

Querys.suit.GetFilePopList = \
SELECT \
FILEID,AFFAIRNUM,TO_CHAR(PRESENTDAY,'YYYY-MM-DD') AS PRESENTDAY,CONTENT,FILENAME, \
FILELOC,REQNAME,REQDAY,REGISTDATE,FILECATEG \
FROM LAWSUIT_FILE WHERE AFFAIRNUM = ? ORDER BY FILEID DESC \

Querys.suit.SetOwnerPopDel = \
DELETE FROM OWNER WHERE AFFAIRNUM = ? AND OWNERNUM = ? AND PNU = ? \

Querys.suit.SetOwnerPopAdd = \
INSERT INTO OWNER(OWNERNUM,AFFAIRNUM,PNU,NAME,JUMINNUM,ADDR,TEL,ISFIXED,OWNRATE,ETC,REGISTDATE) \
VALUES((SELECT MAX(TO_NUMBER(OWNERNUM))+1 FROM OWNER),?,?,?,?,?,?,?,?,?,SYSDATE) \

Querys.suit.GetOwnerPopList = \
SELECT * FROM OWNER WHERE AFFAIRNUM = ? AND PNU = ? AND NVL(ISFIXED,'0') = ? ORDER BY OWNERNUM \

Querys.suit.SetSuitorPopAdd = \
INSERT INTO SUITOR(SUITORNUM,AFFAIRNUM,NAME,ADMINSTNAME,JUMINNUM,ADDR,TEL,ETC,ISMAIN,REGISTDATE,GUBUN) \
VALUES((SELECT MAX(TO_NUMBER(SUITORNUM))+1 FROM SUITOR),?,?,?,?,?,?,?,'2',SYSDATE,?) \

Querys.suit.SetSuitorPopDel = \
DELETE FROM SUITOR WHERE AFFAIRNUM = ? AND SUITORNUM = ? AND GUBUN = ? \

Querys.suit.GetPopSuitorList = \
SELECT * FROM SUITOR \
WHERE ISMAIN <> 1 AND AFFAIRNUM = ? AND GUBUN = ? ORDER BY SUITORNUM \

