Querys.suit.SetPopLandSuitDel = \
DELETE FROM LAND_LAWSUIT WHERE AFFAIRNUM = ? AND PNU = ? \

Querys.suit.SetPopLandSuitInsert = \
INSERT INTO LAND_LAWSUIT(AFFAIRNUM,PNU,TOTALAREA,LAWSUITAREA,CATEG,TOTAL_PRICE,REGISTDATE) \
VALUES(?,?,?,?,?,?,SYSDATE) \

Querys.suit.GetPopLandSuitList = \
SELECT AFFAIRNUM,PNU,TOTALAREA,LAWSUITAREA,TOTAL_PRICE, \
       TO_CHAR(REGISTDATE,'YYYY-MM-DD') as REGISTDATE,USE_PRICE,UNFAIRPRICE,PROFITPRICE, \
       (SELECT CODEDESC FROM CODE WHERE CODENUM = '2' AND CODEVALUE = CATEG) AS CATEG, \
       (SELECT GU_NM||' '||BJ_NM FROM REF_DONGCODE WHERE SI_CD||GU_CD||BJ_CD = SUBSTR(PNU,1,10) AND ROWNUM = 1) \
       ||' '||TO_NUMBER(SUBSTR(PNU,12,4))||'-'||TO_NUMBER(SUBSTR(PNU,16,4)) AS LAND_ADDR \
FROM LAND_LAWSUIT \
WHERE AFFAIRNUM= ? AND PNU <> ? \
