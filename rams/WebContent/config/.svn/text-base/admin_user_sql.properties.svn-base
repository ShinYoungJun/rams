Querys.admin.user.GetRamsNoticeList = \
SELECT * FROM NOTICE WHERE SEQNUM LIKE ? AND ROWNUM <4 ORDER BY REGISTDATE DESC \

Querys.admin.user.GetRoadsNoticeList = \
SELECT * \
  FROM BOARD@DOROSLINK \
 WHERE SYSDATE BETWEEN TO_DATE(NOTICE_FROMDATE) AND TO_DATE(NOTICE_TODATE) AND PUBLIC_YN= ?  AND ROWNUM < 4 \
 ORDER BY REGDATE DESC, BOARD_ID DESC \

Querys.admin.user.checkUserId = \
SELECT COUNT(*) \
  FROM TUSER \
 WHERE USERID = ?
 
Querys.admin.user.selectbyUserId = \
SELECT TU.* \
FROM TUSER TU \
WHERE \
TU.USERID = ? \

Querys.admin.user.GetUserList = \
SELECT TTT.* FROM( \
SELECT ROWNUM AS RNUM,TT.* \
FROM ( \
SELECT (SELECT DISTINCT GU_NM FROM REF_DONGCODE RD WHERE RD.SI_CD = '11' AND RD.GU_CD = T.ADMINIST) AS GU_NM,T.* \
FROM TUSER T \
WHERE T.USERID LIKE ? \
AND T.USERNAME LIKE ? \
AND T.ADMINIST LIKE ? \
AND T.ISAPPROVE LIKE ? \
ORDER BY REGISTDATE DESC,USERID ASC \
) TT \
) TTT WHERE TTT.RNUM > (?-1)*? AND ROWNUM <= ? \

Querys.admin.user.GetUserCount = \
SELECT COUNT(*) FROM TUSER T \
WHERE T.USERID LIKE ? \
AND T.USERNAME LIKE ? \
AND T.ADMINIST LIKE ? \
AND T.ISAPPROVE LIKE ? \

Querys.admin.user.SetUserApprove = \
UPDATE TUSER SET ISAPPROVE = ? \
WHERE USERID = ? \

Querys.admin.user.SetUserDelete = \
DELETE FROM TUSER WHERE USERID= ? \

Querys.admin.user.SetUserUpdate = \
UPDATE TUSER SET AUTHURNAME=?,USERNAME=?,PASS=?,JUMINNUM=?,TEL1=?,TEL2=?,TEL3=?,INLINENUM=?,MTEL1=?,MTEL2=?,MTEL3=?,EMAIL=? \
WHERE USERID = ? \

Querys.admin.user.InsertMember = \
INSERT INTO TUSER(USERID, AUTHURNAME, PASS, USERNAME, ADMINIST, PART, INLINENUM, TEL1, TEL2, TEL3, \
MTEL1, MTEL2, MTEL3, EMAIL, ASSIGNWORK, ISAPPROVE, JUMINNUM, REGISTDATE) \
VALUES(?,?,?,?,? ,?,?,?,?,? ,?,?,?,?,? ,?,?,SYSDATE) \
SELECT * FROM TUSER WHERE USERID LIKE ? AND ADMINIST LIKE ? AND ISAPPROVE LIKE ? ORDER BY REGISTDATE \

Querys.admin.user.InsertMember = \
INSERT INTO TUSER(USERID, AUTHURNAME, PASS, USERNAME, ADMINIST, PART, INLINENUM, TEL1, TEL2, TEL3, \
MTEL1, MTEL2, MTEL3, EMAIL, ASSIGNWORK, ISAPPROVE, JUMINNUM, REGISTDATE) \
VALUES(?,?,?,?,? ,?,?,?,?,? ,?,?,?,?,? ,?,?,SYSDATE) \
