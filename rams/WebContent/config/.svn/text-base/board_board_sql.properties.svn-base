Querys.board.board.BoardInsert = \
INSERT INTO BOARD(SEQNUM,TITLE,CONTENT,REGISTDATE,VIEWCNT,REGISID,REGISTNAME) \
VALUES(BOARD_SEQ.NEXTVAL,?,?,SYSDATE,'0',?,?)

Querys.board.board.BoardList = \
SELECT * \
FROM \
    ( \
    SELECT ROWNUM as RNUM,T1.* \
    FROM ( \
        SELECT \
        SEQNUM,TITLE,CONTENT,TO_CHAR(REGISTDATE,'YYYY-MM-DD') AS REGISTDATE,VIEWCNT,REGISID,REGISTNAME \
        FROM BOARD \
        WHERE \
        NVL(TITLE,' ') LIKE ? AND NVL(CONTENT,' ') LIKE ? AND NVL(REGISTNAME,' ') LIKE ? \
        ORDER BY SEQNUM DESC) T1 \
        ) \
WHERE RNUM > (?-1)*? AND ROWNUM <= ?

Querys.board.board.BoardListCount = \
SELECT \
COUNT(*) \
FROM BOARD \
WHERE \
NVL(TITLE,' ') LIKE ? AND NVL(CONTENT,' ') LIKE ? AND NVL(REGISTNAME,' ') LIKE ?

Querys.board.board.BoardView = \
SELECT SEQNUM,TITLE,CONTENT,TO_CHAR(REGISTDATE,'YYYY-MM-DD') AS REGISTDATE,VIEWCNT,REGISID,REGISTNAME \
FROM BOARD WHERE SEQNUM = ?

Querys.board.board.BoardDelete = \
DELETE FROM BOARD WHERE SEQNUM = ?

Querys.board.board.BoardCntUpdate = \
UPDATE BOARD SET VIEWCNT = TO_NUMBER(VIEWCNT)+1 WHERE SEQNUM = ?

Querys.board.board.BoardUpdate = \
UPDATE BOARD SET TITLE = ?,CONTENT = ?, REGISTDATE = SYSDATE WHERE SEQNUM = ?
