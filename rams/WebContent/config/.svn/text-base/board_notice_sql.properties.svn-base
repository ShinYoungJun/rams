Querys.board.notice.NoticeInsert = \
INSERT INTO NOTICE(SEQNUM,TITLE,CONTENT,REGISTDATE,VIEWCNT,USERID,REGISTNAME) \
VALUES(NOTICE_SEQ.NEXTVAL,?,?,SYSDATE,'0',?,?)

Querys.board.notice.NoticeList = \
SELECT * \
FROM \
    ( \
    SELECT ROWNUM as RNUM,T1.* \
    FROM ( \
        SELECT \
        SEQNUM,TITLE,CONTENT,TO_CHAR(REGISTDATE,'YYYY-MM-DD') AS REGISTDATE,VIEWCNT,USERID,REGISTNAME \
        FROM NOTICE \
        WHERE \
        NVL(TITLE,' ') LIKE ? AND NVL(CONTENT,' ') LIKE ? AND NVL(REGISTNAME,' ') LIKE ? \
        ORDER BY SEQNUM DESC) T1 \
        ) \
WHERE RNUM > (?-1)*? AND ROWNUM <= ?

Querys.board.notice.NoticeListCount = \
SELECT \
COUNT(*) \
FROM NOTICE \
WHERE \
NVL(TITLE,' ') LIKE ? AND NVL(CONTENT,' ') LIKE ? AND NVL(REGISTNAME,' ') LIKE ?

Querys.board.notice.NoticeView = \
SELECT SEQNUM,TITLE,CONTENT,TO_CHAR(REGISTDATE,'YYYY-MM-DD') AS REGISTDATE,VIEWCNT,USERID,REGISTNAME \
FROM NOTICE WHERE SEQNUM = ?

Querys.board.notice.NoticeDelete = \
DELETE FROM NOTICE WHERE SEQNUM = ?

Querys.board.notice.NoticeCntUpdate = \
UPDATE NOTICE SET VIEWCNT = TO_NUMBER(VIEWCNT)+1 WHERE SEQNUM = ?

Querys.board.notice.NoticeUpdate = \
UPDATE NOTICE SET TITLE = ?,CONTENT = ?, REGISTDATE = SYSDATE WHERE SEQNUM = ?
