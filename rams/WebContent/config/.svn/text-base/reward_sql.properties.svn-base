FROM( \
  SELECT DISTINCT CP.COMPEID,CP.PNU,CP.GUBUN,RD.GU_NM,RD.BJ_NM,TO_NUMBER(SUBSTR(CP.PNU,12,4)) AS BONBUN,TO_NUMBER(SUBSTR(CP.PNU,16,4)) AS BUBUN, \
