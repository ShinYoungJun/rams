SELECT CNT,PNU,SI_NM,GU_NM,BJ_NM,BONBUN,BUBUN,OWN,REPORT,ROADNAME FROM ( \
SELECT ROWNUM as RNUM, T.* \
FROM (  \
SELECT PNU  \
