) S  \
WHERE (GU_NM LIKE ?) OR (BJ_NM LIKE ?) OR (BONBUN LIKE ?) OR (BUBUN LIKE ?) \
)WHERE RNUM > (?-1)*? AND ROWNUM <= ? 



