AND RD.SI_CD||RD.GU_CD||RD.BJ_CD = SUBSTR(CP.PNU,1,10)) WHERE (GU_NM LIKE ?) OR (BJ_NM LIKE ?) OR (BONBUN LIKE ?) OR (BUBUN LIKE ?)) AS TOTNUM \
,CP.PNU,CP.ISCOMPENSATE,CP.REASON,CP.ETC,CP.REGISTDATE \
,RD.GU_NM,RD.BJ_NM,TO_NUMBER(SUBSTR(CP.PNU,12,4)) AS BONBUN,TO_NUMBER(SUBSTR(CP.PNU,16,4)) AS BUBUN \
,TO_CHAR(CD.INVESTDATE,'YYYY-MM-DD') AS INVESTDATE,CD.INVESTPLACE \
,RDS.INVESTID, RDS.COMPEID \
FROM COMPE_DISCUSS CD, REQ_DISCUSS RDS, COMPENSATION CP, REF_DONGCODE RD \
