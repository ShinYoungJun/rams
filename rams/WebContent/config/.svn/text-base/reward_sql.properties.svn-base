CP.PNU,CP.ISCOMPENSATE,CP.REASON,CP.ETC,CP.REGISTDATE \
,RD.GU_NM,RD.BJ_NM,TO_NUMBER(SUBSTR(CP.PNU,12,4)) AS BONBUN,TO_NUMBER(SUBSTR(CP.PNU,16,4)) AS BUBUN \
