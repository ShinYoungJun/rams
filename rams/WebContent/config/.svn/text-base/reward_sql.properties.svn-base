Querys.reward.getComepeView = \
SELECT DISTINCT CP.COMPEID,CP.PNU,CT.GOSIID,CT.CONSTNUM,CP.LANDGUBUN,CP.LANDDETAIL, \
       RD.GU_NM,RD.GU_CD,RD.BJ_NM,RD.BJ_CD,TO_NUMBER(SUBSTR(CP.PNU,12,4)) AS BONBUN,TO_NUMBER(SUBSTR(CP.PNU,16,4)) AS BUBUN, \
       (SELECT CODEDESC FROM CODE WHERE CODENUM='2' AND CODEVALUE =CP.JIMOK ) AS JIMOK, \
       CP.TOTAREA,CP.AFFAIRAREA,CP.REASON,CP.UNITPRICE,CP.TOTPRICE,CN.NAME,CN.JUMINNUM,CN.ADDRESS, \
       CJ1.OFFICENAME AS OFFICENAME_1,CJ1.DANGA AS DANGA_1,CJ1.JUDGEPRICE AS JUDGEPRICE_1,TO_CHAR(CJ1.POINTDATE,'YYYY-MM-DD') AS POINTDATE_1, \
       CJ2.OFFICENAME AS OFFICENAME_2,CJ2.DANGA AS DANGA_2,CJ2.JUDGEPRICE AS JUDGEPRICE_2,TO_CHAR(CJ2.POINTDATE,'YYYY-MM-DD') AS POINTDATE_2, \
       CR.REQDATE, CR.RECEIPT, CP.JIMOK as JIMOK_CODE \
FROM COMPE_REQ CR,COMPENSATION CP,CONSTRUCT CT,COMPE_PERSON CN,REF_DONGCODE RD, \
(SELECT COMPEID,OFFICENAME,SUM(DANGA) AS DANGA, SUM(JUDGEPRICE) AS JUDGEPRICE,POINTDATE FROM COMPE_JUDGE WHERE JUDGEGUBUN='1' GROUP BY COMPEID, OFFICENAME, POINTDATE) CJ1, \
(SELECT COMPEID,OFFICENAME,SUM(DANGA) AS DANGA, SUM(JUDGEPRICE) AS JUDGEPRICE,POINTDATE FROM COMPE_JUDGE WHERE JUDGEGUBUN='2' GROUP BY COMPEID, OFFICENAME, POINTDATE) CJ2 \
WHERE CP.COMPEID = ? AND CP.PNU = ? \
AND CR.COMPEID = CP.COMPEID AND CR.CONSTNUM = CT.CONSTNUM \
AND CR.COMPEID = CN.COMPEID \
AND CP.COMPEID = CJ1.COMPEID(+) \
AND CP.COMPEID = CJ2.COMPEID(+) \
AND RD.SI_CD||RD.GU_CD||RD.BJ_CD = SUBSTR(CP.PNU,1,10)

