Querys.reward.GetRewardAnalList = \
SELECT \
    T.GU_CD,T.GU_NM, \
    COUNT(T.COMPEID) AS TOTAL_COMPE, \
    COUNT(CASE WHEN NVL(T.ISCOMPENSATE,'N') = 'Y' THEN T.COMPEID end) AS COMPE, \
    COUNT(CASE WHEN NVL(T.ISCOMPENSATE,'N') = 'N' THEN T.COMPEID end) AS UN_COMPE, \
    SUM(T.AFFAIRAREA) AS AREA, \
    SUM(T.TOTPRICE) AS PRICE \
FROM ( \
    SELECT \
    SUBSTR(C.PNU,3,3) AS GU_CD, \
    (SELECT DISTINCT GU_NM FROM REF_DONGCODE WHERE SI_CD||GU_CD||BJ_CD = SUBSTR(C.PNU,1,10)) AS GU_NM, \
    C.COMPEID, \
    C.PNU, \
    C.AFFAIRAREA, \
    C.ISCOMPENSATE, \
    C.TOTPRICE \
    FROM \
        COMPE_REQ CR,COMPENSATION C \
    WHERE \
        CR.COMPEID = C.COMPEID \
        AND NVL(CR.REQDATE,'00000000') >= ? \
        AND NVL(CR.REQDATE,'00000000') <= ? \
        AND NVL(TO_CHAR(CR.REGISTDATE,'YYYYMMDD'),'00000000') >= ? \
        AND NVL(TO_CHAR(CR.REGISTDATE,'YYYYMMDD'),'00000000') <= ? \
) T \
GROUP BY T.GU_CD,T.GU_NM \
ORDER BY T.GU_CD \

Querys.reward.getRewardList = \
SELECT * FROM \
( \
SELECT ROWNUM as RNUM ,T.* \
FROM( \
  SELECT DISTINCT CR.COMPEID,CP.PNU,CS.LINENAME,CS.CONSTNUM, \
         RD.GU_NM,RD.BJ_NM,TO_NUMBER(SUBSTR(CP.PNU,12,4)) AS BONBUN,TO_NUMBER(SUBSTR(CP.PNU,16,4)) AS BUBUN, \
         CS.CONSTNAME,(SELECT CODEDESC FROM CODE WHERE CODENUM='2' AND CODEVALUE =CP.JIMOK ) AS JIMOK, CP.TOTAREA, CP.AFFAIRAREA, \
         CP.ISCOMPENSATE,CP.REASON,TO_CHAR(CP.REGISTDATE,'YYYY-MM-DD') AS REGISTDATE,CP.ETC \
  FROM COMPE_REQ CR,CONSTRUCT CS,COMPENSATION CP,REF_DONGCODE RD \
  WHERE CR.CONSTNUM = CS.CONSTNUM \
  AND CR.COMPEID = CP.COMPEID \
  AND RD.SI_CD||RD.GU_CD||RD.BJ_CD = SUBSTR(CP.PNU,1,10) \
  ORDER BY REGISTDATE DESC \
) T \
WHERE (T.GU_NM LIKE ?) OR (T.BJ_NM LIKE ?) OR (T.BONBUN LIKE ?) \
OR (T.BUBUN LIKE ?) OR (T.CONSTNUM LIKE ?) OR (T.CONSTNAME LIKE ?) \
) WHERE RNUM > (?-1)*? AND ROWNUM <= ?

