WHERE ((T.GU_NM LIKE ?) OR (T.BJ_NM LIKE ?) OR (T.BONBUN||'-'||T.BUBUN LIKE ?)) \
      AND NVL(TO_CHAR(T.REGISTDATE,'YYYYMMDD'),'00000000') >= ? \
