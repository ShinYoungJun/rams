Querys.reward.GetRewardOwnList = \
SELECT OWNGUBUN,CNT,AREA,JIGA FROM ( \
SELECT OWNGUBUN   \
,COUNT(PNU) AS CNT   \
,SUM(AREA) AS AREA   \
,SUM(JIGA) AS JIGA   \
FROM T_PNU   \
WHERE OWNGUBUN IN ('1','2','4','5')   \
AND ROADNAME IN (?)  \
GROUP BY OWNGUBUN   \
ORDER BY OWNGUBUN ASC) T   \
UNION ALL   \
SELECT 'ETC' AS OWNGUBUN    \
,(SELECT COUNT(*) FROM (SELECT * FROM T_PNU WHERE OWNGUBUN NOT IN ('1','2','4','5') OR OWNGUBUN IS NULL ) WHERE ROADNAME IN (?)) AS CNT \
,SUM(T.AREA) AS AREA    \
,SUM(T.JIGA) AS JIGA    \
FROM (    \
SELECT OWNGUBUN    \
,SUM(AREA) AS AREA    \
,SUM(JIGA) AS JIGA  \
FROM ( \
SELECT * \
FROM T_PNU    \
WHERE OWNGUBUN NOT IN ('1','2','4','5')  \
OR OWNGUBUN IS NULL ) \
WHERE ROADNAME IN (?)   \
GROUP BY OWNGUBUN    \
)T GROUP BY 'ETC' \
UNION ALL   \
SELECT 'TOTAL' AS OWNGUBUN_NM   \
,COUNT(PNU) AS CNT   \
,SUM(AREA) AS AREA   \
,SUM(JIGA) AS JIGA   \
FROM T_PNU  \
WHERE ROADNAME IN (?) 

