Querys.suit.SetLawSuitCopy = \
INSERT INTO LAWSUIT(AFFAIRNUM,AFFAIRNAME,UNITCOST,LAWSUITPRICE,CALSS,JUDGMENT,COURT,ACCEPTDATE,CLOSEDAY,ANNOUNCEDATE,OUTCOME,RESULT,CONSTNUM, \
       PRELAWSUITNUM,REGISTDATE,USERID,LAWGUBUN,SUITORPRICE,DEFENDENTPRICE,GUCODE,LINENAME,UNFAIRPROFIT) \
SELECT ?,?,UNITCOST,LAWSUITPRICE,?,?,?,?,?,'','','',CONSTNUM, \
       ?,SYSDATE,?,?,SUITORPRICE,DEFENDENTPRICE,GUCODE,LINENAME,UNFAIRPROFIT \
FROM LAWSUIT \
WHERE AFFAIRNUM = ? \

Querys.suit.SetSuitorCopy = \
INSERT INTO SUITOR(SUITORNUM,AFFAIRNUM,NAME,ADMINSTNUM,ADMINSTNAME,JUMINNUM,ADDR,TEL,ETC,ISMAIN,ISDEL,REGISTDATE,GUBUN) \
SELECT \
    (SELECT MAX(TO_NUMBER(SUITORNUM)) FROM SUITOR)+ROWNUM,?,NAME,ADMINSTNUM, \
    ADMINSTNAME,JUMINNUM,ADDR,TEL,ETC,ISMAIN,ISDEL,SYSDATE,GUBUN FROM SUITOR \
WHERE AFFAIRNUM = ? \

